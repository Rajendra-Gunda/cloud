pipeline {
    agent any
    stages{
        stage('git'){
            
            steps {git branch: 'main', url: 'https://github.com/Rajendra-Gunda/cloud.git'}
        }

        stage('jenkins_sync'){
            
            agent{
                label "jenkins"
            }
            steps {
            sh 'rsync -avh /var/lib/jenkins/workspace/cloud/jenkinsfile jenkins@172.31.7.165:/opt'
            }
        }
    }
}
