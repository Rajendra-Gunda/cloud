pipeline {
    agent {'jenkins'}
    stages{
        stage('git'){
            
            steps {git branch: 'main', url: 'https://github.com/Rajendra-Gunda/cloud.git'}
        }

        stage('jenkins_sync'){
            
            agent{
                label "jenkins"
            }
            steps {
            sh 'rsync -avh /var/lib/jenkins/workspace/cloudknowledge/Dockerfile jenkins@10.1.1.11:/opt'
            }
        }
    }
}
